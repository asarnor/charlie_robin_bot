# Production Environment Configuration
# WARNING: This file contains sensitive credentials. Never commit to version control!
# Copy this file to .env.production and fill in your actual values
ENVIRONMENT=production

# Trading Bot Configuration
WATCHLIST=SPY,QQQ,TSLA,NVDA,AAPL,MSFT
OPTIONS_WATCHLIST=SPY,QQQ
MAX_DRAWDOWN_PCT=0.10
WASH_SALE_DAYS=31
CYCLE_INTERVAL_SECONDS=900
LOG_FILE=bot_state_production.json

# Charles Schwab Configuration (Production)
SCHWAB_ENABLED=false
SCHWAB_API_KEY=your_schwab_production_api_key_here
SCHWAB_APP_SECRET=your_schwab_production_app_secret_here
SCHWAB_CALLBACK_URL=https://your-production-domain.com/callback
SCHWAB_TOKEN_PATH=./schwab_token_production.json
SCHWAB_ACCOUNT_ID=your_production_account_id
SCHWAB_BASE_URL=https://api.schwabapi.com/v1

# Robinhood Configuration (Production)
ROBINHOOD_ENABLED=false
ROBINHOOD_USERNAME=your_robinhood_production_username
ROBINHOOD_PASSWORD=your_robinhood_production_password
ROBINHOOD_MFA_CODE=
ROBINHOOD_API_KEY=
ROBINHOOD_DEVICE_TOKEN=

